<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles.css">
    <title>Grades API</title>
</head>
<body>
    <div class="container">
        <h1>Grades API</h1>
        <input type="text" id="searchInput" placeholder="Search by ObjectId">
        <button id="fetchButton">Fetch Grade</button>

        <table id="gradesTable">
            <thead>
                <tr>
                    <th>ObjectId</th>
                    <th>Scores</th>
                    <th>Class ID</th>
                    <th>Student ID</th>
                </tr>
            </thead>
            <tbody id="gradesList"></tbody>
        </table>
    </div>

    <script src="/script.js"></script>
</body>
</html>
